{
    "manifest_version": 2,
    "name": "Lista de Presença Meets",
    "description" : "Extensão para gerar uma lista de presença",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
                ],
            "css": [
                "style.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup/popup.html"
    }
}