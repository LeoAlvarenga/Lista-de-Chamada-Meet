{
    "manifest_version": 2,
    "name": "MeetsChamada",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
                ],
            "css": [
                "style.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup/popup.html"
    }
}